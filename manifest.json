{
  "manifest_version": 3,
  "name": "Cartman AI",
  "version": "1.0",
  "description": "Chat with you while you are browsing a website.",
  "icons": {
   
  },
  "side_panel": {
    "default_path": "sidepanel/index.html"
  },
  "permissions": ["sidePanel","activeTab","scripting"],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches":["<all_urls>"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker":"scripts/background.js"
  }
}